{
  "extends": ["airbnb","prettier"],
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "react/prop-types": 0,,
    "react/require-default-props": 0,
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
